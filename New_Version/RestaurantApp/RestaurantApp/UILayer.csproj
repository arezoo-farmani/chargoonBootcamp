<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows10.0.17763.0</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="BOLayer\**" />
    <Compile Remove="RepositoryLayer\**" />
    <Compile Remove="Service\**" />
    <EmbeddedResource Remove="BOLayer\**" />
    <EmbeddedResource Remove="RepositoryLayer\**" />
    <EmbeddedResource Remove="Service\**" />
    <None Remove="BOLayer\**" />
    <None Remove="RepositoryLayer\**" />
    <None Remove="Service\**" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="BOLayer\BOLayer.csproj" />
    <ProjectReference Include="Service\ServiceLayer.csproj" />
  </ItemGroup>

</Project>